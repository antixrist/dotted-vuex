{
  "name": "dotted-vuex",
  "version": "1.0.2",
  "description": "Better Vuex without types, simple and fast to use (dotted names and autoprefixed methods).",
  "main": "dist/dotted-vuex.js",
  "scripts": {
    "build": "npm run build:main",
    "build:main": "rollup -c build/rollup.config.js && uglifyjs dist/dotted-vuex.js -cm --comments -o dist/dotted-vuex.min.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happierall/dotted-vuex.git"
  },
  "keywords": [
    "dotted",
    "vuex",
    "easy",
    "autoprefix",
    "vue.js",
    "huge",
    "large",
    "project"
  ],
  "author": "Ruslan Yanberdin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/happierall/dotted-vuex/issues"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.1.18",
    "babel-runtime": "^6.0.0",
    "rollup": "^0.34.13",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-watch": "^2.5.0",
    "uglify-js": "^2.6.2"
  },
  "homepage": "https://github.com/happierall/dotted-vuex#readme"
}
